<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite default-match-type="wildcard">
	<rule>
		<from>/images/**</from>
		<to>/images/$1</to>
	</rule>
	<rule>
		<from>/scripts/**</from>
		<to>/scripts/$1</to>
	</rule>
	<rule>
		<from>/styles/**</from>
		<to>/styles/$1</to>
	</rule>
	<rule>
		<from>/sitemap/**</from>
		<to>/sitemap/$1</to>
	</rule>
	<rule>
		<from>/robots.txt</from>
		<to>/public/robots.txt</to>
	</rule>
	<rule>
		<from>/resources/**</from>
		<to>/resources/$1</to>
	</rule>
	<rule>
		<from>/spring/javax.faces.resource/**</from>
		<to>/faces/javax.faces.resource/$1</to>
	</rule>
	<rule>
		<from>/**</from>
		<to>/spring/$1</to>
	</rule>
	<outbound-rule>
		<from>/spring/.*</from>
		<to>/$1</to>
	</outbound-rule>
</urlrewrite>